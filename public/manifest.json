{
    "manifest_version": 3,
    "name": "OG Insights",
    "version": "1.0.0",
    "description": "OG Insights provides AI-generated analysis directly on the webpage.",
    "permissions": ["tabs", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "insight_2644450.png",
        "48": "insight_2644450.png",
        "128": "insight_2644450.png"
      }
    },
    "icons": {
      "16": "insight_2644450.png",
      "48": "insight_2644450.png",
      "128": "insight_2644450.png"
    }
  }
  